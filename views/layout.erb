<!DOCTYPE>
<html>

  <head>
    <title><%= BLOG_TITLE %> | <%= @title %></title>
    <link rel="alternate" type="application/rss+xml" title="RSS 2.0" href="http://feeds.feedburner.com/codebiff">
    <link rel="stylesheet" href="/css/main.css">
    <link rel="shortcut icon" href="/favicon.ico">
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  </head>

  <body>
    
    <section id="container">




      <section id="mainColumn">
        
        <header>
          <a href="/"><img src="/images/title.png" alt="CodeBiff"></a>
        </header>

        <section id="contents">
          
          <%= yield %>

        </section> <!-- /contents -->



      </section>






      <section id="sidebar">

        <section class="sidebar-box">
          <a href="http://feeds.feedburner.com/codebiff" class="social-link"><img src="/images/rss.png" alt="rss feed"></a>  
          <a href="https://twitter.com/#!/codebiff" class="social-link"><img src="/images/twitter.png" alt="twitter"></a>  
        </section> <!-- /sidebar-box -->

        <section class="sidebar-box">
          <h3>tags</h3>
          <ul class="tags">
            <% for tag in @tags %>
              <li><a href="/tags/<%= tag %>"><%= tag %></a></li>
            <% end %>
          </ul>
        </section> <!-- /sidebar-box -->

      </section>





      <footer id="footer">
      <p>
        <a rel="license" href="http://creativecommons.org/licenses/by-nc-sa/3.0/"><img alt="Creative Commons Licence" style="border-width:0" src="http://i.creativecommons.org/l/by-nc-sa/3.0/80x15.png" /></a><br />This work is licensed under a <a rel="license" href="http://creativecommons.org/licenses/by-nc-sa/3.0/">Creative Commons Attribution-NonCommercial-ShareAlike 3.0 Unported License</a>.
      </p>
      </footer>

    </section> <!-- /container -->

    <script type="text/javascript">

      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-28186072-1']);
      _gaq.push(['_trackPageview']);

      (function() {
         var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
         ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
         var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
       })();

    </script>
  </body>
</html>
